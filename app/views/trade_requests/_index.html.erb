<div class="row">
	<div class="col-md-12">
		<div class="table-responsive">
			<table class="table table-striped">
				<caption>Trade History</caption>
				<tr>
					<th>Date</th>
					<th>Offerer</th>
					<th>Receiver</th>
					<th>Response</th>
					<th>Response Date</th>
				</tr>
				<%= render @trade_requests %>
			</table>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-md-4 col-md-offset-4">
		<%= will_paginate @trade_requests, renderer: BootstrapPagination::Rails %>
	</div>
</div>